	LINK=
	SYMBOL=.\debug\ThumbCode.elf
	DOWNLOAD=.\debug\ThumbCode.elf
	DOWNLOADADDR=0x0c000000
	RESETSYMBOL=
	SCRIPT=..\common\ev40boot.cs
	<END>

	<BEGIN> DIR
	SUBDIR=FALSE
	<END>

<END>

<BEGIN> BUILD_CONFIG
	CONFIG_DLL=GccArmSettingPage.dll
	<BEGIN> COMPILE_COMMAND_LINE
	-gdwarf -c $(SOURCE_FILE) -o.\debug\$(OBJ_FILE)
	<END>
	<BEGIN> ASSEMBLE_COMMAND_LINE
	-gdwarf2 $(SOURCE_FILE) -o.\debug\$(OBJ_FILE)
	<END>
	<BEGIN> LINK_COMMAND_LINE
	arm-elf-ld -Ttext 0x0C000000 -o.\debug\ThumbCode.elf $(<ThumbCode.o>OBJ_FILES)
	<END>
<END>

<BEGIN> PROJECT_FILES

	<BEGIN> GROUP="Project Source Files"
		<PROP> GROUP_FILTER="*.c;*.cpp;*.cc;*.cxx;*.s;*.asm"

		<BEGIN> SOURCE_FILE
		SOURCE=.\ThumbCode.s
		STATUS=CLOSE
		<END>

	<END>

	<BEGIN> SOURCE_FILE

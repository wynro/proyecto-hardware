<BEGIN> PROJECT_NAME
ThumbCode
<END>

<BEGIN> CONFIG

	<BEGIN> CPU
	MODULE=arm7.DLL
	FAMILY=ARM7
	FAMILYID=1
	MEMBER=ARM7
	MEMBERID=1
	ENDIAN=0
	<END>

	<BEGIN> REMOTE
	DEVICE=SimArm7.DLL
	COMMTYPE=PARALLEL
	PARALLEL=LPT1
	SPEED=0
	<END>

	<BEGIN> DEBUG
	AUTODOWNLOAD=FALSE
	AUTOSCRIPT=TRUE
	VERIFY=TRUE
	USEMEMMAP=FALSE
	EXECFDOWN=FALSE
